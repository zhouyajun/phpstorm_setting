<templateSet group="bootstrap-my">
  <template name="b-panel" value="&lt;div class=&quot;panel panel-default$END$&quot;&gt;&#10;  &lt;div class=&quot;panel-heading&quot;&gt;&#10;    &lt;h3 class=&quot;panel-title&quot;&gt;Panel title&lt;/h3&gt;&#10;  &lt;/div&gt;&#10;  &lt;div class=&quot;panel-body&quot;&gt;&#10;    Panel content&#10;  &lt;/div&gt;&#10;  &lt;div class=&quot;panel-footer&quot;&gt;Panel footer&lt;/div&gt;&#10;&lt;/div&gt;" description="panel" toReformat="false" toShortenFQNames="true">
    <context>
      <option name="HTML" value="true" />
      <option name="PHP" value="true" />
      <option name="Vue" value="true" />
    </context>
  </template>
  <template name="b-input" value="&lt;input type=&quot;$TYPE$&quot; class=&quot;form-control&quot; id=&quot;$ID$&quot; placeholder=&quot;$PLACEHOLDER$&quot;&gt;" description="input" toReformat="false" toShortenFQNames="true">
    <variable name="TYPE" expression="enum(&quot;text&quot;,&quot;radio&quot;,&quot;checkbox&quot;,&quot;hidden&quot;,&quot;button&quot;)" defaultValue="text" alwaysStopAt="true" />
    <variable name="ID" expression="camelCase(&quot;ID&quot;)" defaultValue="ID" alwaysStopAt="true" />
    <variable name="PLACEHOLDER" expression="camelCase(&quot;PLACEHOLDER&quot;)" defaultValue="PLACEHOLDER" alwaysStopAt="true" />
    <context>
      <option name="HTML" value="true" />
      <option name="PHP" value="true" />
    </context>
  </template>
  <template name="b-table" value="&lt;!-- table-responsive 响应式表哥 --&gt;&#10;&lt;div class=&quot;table-responsive&quot;&gt;&#10;&lt;table class=&quot;table table-hover table-bordered table-striped $END$&quot;&gt;&#10;       &lt;tr&gt;&#10;        &lt;th&gt;&lt;/th&gt;&#10;        &lt;th&gt;&lt;/th&gt;&#10;       &lt;/tr&gt;&#10;       &lt;tr&gt;&#10;        &lt;td&gt;&lt;/td&gt;&#10;        &lt;td&gt;&lt;/td&gt;&#10;       &lt;/tr&gt;&#10;&lt;/table&gt;&#10;&lt;/div&gt;" description="bootstrap表格" toReformat="false" toShortenFQNames="true">
    <context>
      <option name="HTML" value="true" />
      <option name="PHP" value="true" />
    </context>
  </template>
</templateSet>